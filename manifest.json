{
    "manifest_version": 2,
    "name": "definer",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["index.js"]
      }
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": ["tabs"],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "main/index.html",
      "default_title": "definer"
    }
  }