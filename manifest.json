{
    "manifest_version": 2,
    "name": "Definer",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["main.js"]
      }]
    ,
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "index.html",
      "default_title": "Definer"
    }
  }
  